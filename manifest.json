{
  "name": "Charlie Parker",
  "version": "0.1",
  "manifest_version": 1,
  "description": "Listen to your browser make requests.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["functions.js", "popup.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}