{
  "name": "Aleatoric",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Listen to your browser make requests.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "aleatoric-icon-16.png",
    "48": "aleatoric-icon-48.png",
    "128": "aleatoric-icon-128.png"
  },
  "background": {
    "scripts": ["functions.js", "charlie.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}